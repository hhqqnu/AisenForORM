<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.m.sqlite.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />
    
    <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <!-- <uses-permission android:name="android.permission.WRITE_GSERVICES"/> -->
    <!-- 往SDCard写入数据权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!--加载被测主程序包名 -->
    <instrumentation android:name="android.test.InstrumentationTestRunner" android:targetPackage="com.m.sqlite.sample"/>
    
    <application android:name="com.m.sqlite.sample.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <!--需要加载测试扩展包 -->
        <uses-library android:name="android.test.runner"/>
        
        <activity android:name="com.m.sqlite.sample.SampleActivity" 
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
